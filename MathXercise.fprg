    <function name="loading" type="None" variable="">
        <parameters/>
        <body>
            <declare name="nilai" type="Integer" array="False" size=""/>
            <declare name="delay" type="Integer" array="False" size=""/>
            <assign variable="nilai" expression="0"/>
            <while expression="nilai = 0">
                <output expression="&quot; &quot;" newline="False"/>
                <for variable="delay" start="0" end="40000" direction="inc" step="1"/>
                <assign variable="nilai" expression="1"/>
            </while>
        </body>
    </function>

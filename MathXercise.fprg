    <function name="minlvl1" type="None" variable="">
        <parameters>
            <parameter name="nama" type="String" array="True"/>
        </parameters>
        <body>
            <declare name="nyawa" type="Integer" array="False" size=""/>
            <declare name="tebakan" type="Integer" array="False" size=""/>
            <declare name="random1, random2" type="Integer" array="False" size=""/>
            <declare name="jawaban" type="Integer" array="False" size=""/>
            <assign variable="random1" expression="random(11)"/>
            <assign variable="random2" expression="random(10)"/>
            <assign variable="nyawa" expression="4"/>
            <while expression="random1 &lt; random2">
                <assign variable="random1" expression="random(11)"/>
                <if expression="random1 &gt; random2">
                    <then/>
                    <else>
                        <assign variable="random2" expression="9"/>
                    </else>
                </if>
            </while>
            <assign variable="jawaban" expression="random1 - random2"/>
            <declare name="skor" type="Integer" array="False" size=""/>
            <assign variable="skor" expression="0"/>
            <output expression="&quot;AREA SUNGAI PENGURANGAN&quot; &amp; ToChar(13) &amp; &#13;&#10;&quot;========================&quot; &amp; ToChar(13) &amp;&#13;&#10;&quot;LEVEL 1  *MONSTER KATAK*&quot;" newline="True"/>
            <call expression="loading"/>
            <output expression="&quot;No.1)  &quot; &amp; random1 &amp; &quot; - &quot; &amp; random2 &amp; &quot; = ?&quot;" newline="True"/>
            <input variable="tebakan"/>
            <while expression="tebakan != jawaban &amp;&amp; nyawa &gt; 0">
                <if expression="tebakan != jawaban">
                    <then>
                        <output expression="&quot;Jawabanmu salah&quot; &amp; ToChar(13) &amp;&#13;&#10;&quot;Kamu masih punya &quot; &amp; nyawa &amp; &quot; kesempatan lagi&quot;" newline="True"/>

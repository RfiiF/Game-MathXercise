                    </then>
                    <else/>
                </if>
                <if expression="tebakan = jawaban">
                    <then>
                        <output expression="&quot;Jawaban mu BENAR&quot;" newline="True"/>
                        <call expression="loading&#13;&#10;"/>
                    </then>
                    <else>
                        <output expression="&quot;Ayo hitung lagi dengan tepat&quot;" newline="True"/>
                        <output expression="&quot;No.1)  &quot; &amp; random1 &amp; &quot; - &quot; &amp; random2 &amp; &quot; = ?&quot;" newline="True"/>
                        <input variable="tebakan"/>
                        <assign variable="nyawa" expression="nyawaMin(nyawa)"/>
                    </else>
